{
  "name": "ninsho-module-secure",
  "version": "0.0.17",
  "description": "security-related module used on ninsho.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "scripts": {
    "lint": "eslint \"src/**/*.ts\" \"src/*.ts\"",
    "prebuild": "rimraf dist",
    "test": "jest --config ./jest.config.json --runInBand --coverage",
    "build-t": "tsc",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build": "npm run build:cjs && npm run build:esm"
  },
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "dist"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ninsho/ninsho-module-secure/issues"
  },
  "homepage": "https://github.com/ninsho/ninsho-module-secure/",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "ninsho-base": "^0.0.9",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^29.4.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "eslint": "^7.8.1",
    "jest": "^29.5.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  },
  "sideEffects": false
}
